@using OrderTracker.Models;

<h3>Orders from @Vendors.find(@Model["vendor"]).Name</h3>

<ol>
@foreach (Order order in @Model["orders"])
{
  <li><a href='/vendors/@Model["vendor"].Id/orders/@order.Id'>@order.Date</a></li>
}
</ol>

<p><a href='/vendors/@Model["vendor"].Id/orders/new'>Add a new Order</a></p>
<p><a href='/'>Return to Vendors list</a></p>
<p><a href='/'>Return to Main Page</a></p>